<?xml version="1.0" encoding="UTF-8"?>
<project name="serverbutton" default="firefox" basedir=".">

	<target name="firefox">
		<echo message="Building firefox installation file" />
		<zip destfile="serverbutton.xpi" basedir="firefox" excludes="test" />
	</target>

	<target name="clean">
		<delete file="serverbutton.xpi" />
	</target>

</project>
